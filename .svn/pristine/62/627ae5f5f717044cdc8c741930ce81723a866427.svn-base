<app-form-neiran *ngIf="formLoaded; else loading" [formGroup]="formGroup" (submitEvent)="onSubmit($event)"
[disableControl]="disableControl"
[formTitle]="'EDITAR COMANDO'"
(closeEvent)="onClose($event)"></app-form-neiran>

<ng-template #loading>
    <div class="modal-header">
    <div class="col-12">
    <mat-progress-bar mode="indeterminate"></mat-progress-bar>
    </div>
    </div>
  </ng-template>